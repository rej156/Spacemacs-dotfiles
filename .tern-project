{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "jquery"
  ],
  "loadEagerly": [
    "path/to/your/js/**/*.js",
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "dontLoad": [
    "path/to/your/js/**/*.js"
  ],
  "plugins": {
    "complete_strings": {},
    "node": {},
    "node-express": {},
    "lint": {},
    "angular": {},
    "commonjs": {},
    "requirejs": {},
    "modules": {},
    "es_modules": {},
    "~/tern-react": {},
    "doc_comment": {
      "fullDocs": true
    }
  }
}