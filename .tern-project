{
  "dependencyBudget": "tern.defaultOptions.dependencyBudget",
  "ecmaScript": true,
  "ecmaVersion": 6,
  "esVersion": 6,
  "libs": [
    "ecma6",
    "ecma5",
    "browser",
    "jquery",
    "jest"
  ],
  "loadEagerly": [
    "./src/**",
    "./src/**/*.js",
    "./src/**/*.jsx",
    "./src/**/*.test.jsx",
    "./src/**/*.test.js",
    "./src/**/*.spec.jsx",
    "./src/**/*.spec.js",
    "./node_modules/react/**/*.js",
    "path/to/your/js/**/*.js",
    "*.js",
    "*.jsx"
  ],
  "dontLoad": [
    "path/to/your/js/**/*.js",
    "node_modules/**"
  ],
  "plugins": {
    "complete_strings": {},
    "node": {},
    "node-extension": {},
    "node-express": {},
    "lint": {},
    "angular": {},
    "commonjs": {},
    "requirejs": {},
    "modules": {},
    "es_modules": {},
    "react": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
